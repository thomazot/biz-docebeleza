
/**
 * Mixins
 */
icon(i)
    content: i
    text-transform: none
    font-weight: normal
    font-style: normal
    font-variant: normal
    font-family: 'biz_icons'
    speak: none
    -moz-osx-font-smoothing: grayscale
    -webkit-font-smoothing: antialiaseds

/**
 * Mixin - Font-weight
 */
font = 'Source Sans Pro', sans-serif
light = 300
regular = 400
medium = 500
semibold = 600
bold = 700
/**
 * Mixin - Viewport
 */
viewport = 1200px
gap = 32px
/**
 * Mixin - Responsive
 * @media (max-width screen-sm-min)
 * @media (max-width screen-md-min)
 * @media (max-width screen-lg-min)
 */
screen-xxs = 320px
// Small devices (phones, 568px and down)
screen-xs = 568px
// Small devices (tablets, 768px and down)
screen-sm = 768px
// Medium devices (desktops, 992px and down)
screen-md = 992px
// Medium devices (desktops, 1200px and down)
screen-lg = 1200px

$container
    margin-right: auto
    margin-left: auto
    max-width: 'calc(100% - %s)' % gap
    width: viewport

white = #ffffff
black = #000
gray = #707070
